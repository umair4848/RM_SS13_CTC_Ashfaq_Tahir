<?xml version="1.0"?>
<launch>

    <arg name="robot_env" default="$(optenv ROBOT_ENV !!NO_ROBOT_ENV_SET!!)"/>

    <rosparam command="load" ns="/script_server/base" file="$(find hbrs_default_env_config)/$(arg robot_env)/navigation_goals.yaml"/>
    <rosparam command="load" ns="/script_server/base_orientations" file="$(find hbrs_default_env_config)/$(arg robot_env)/navigation_orientations.yaml"/>
    <rosparam command="load" ns="/simulation/objects" file="$(find hbrs_default_env_config)/$(arg robot_env)/simulation_object_locations.yaml"/>

</launch>
